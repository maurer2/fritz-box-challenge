  "main": "index.js",
  "scripts": {
    "build": "node scripts/build.js",
    "dump:box-data": "ts-node ./scripts/dump-box-data.ts",
    "lint:style": "stylelint 'src/**/*.{css,js,jsx}'; exit 0",
    "lint:js": "eslint 'libs/**/*.{js,jsx}'",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint:ts": "eslint 'src/**/*.{ts,tsx}'",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "format": "prettier --write './**/*.{ts,tsx,js,jsx,json}'",
    "format-and-lint": "npm run format && npm run lint:style && npm run lint:ts:fix",
    "start": "BROWSER=none REACT_APP_MODE=live node scripts/start.js",
    "start:demo": "BROWSER=none REACT_APP_MODE=dev node scripts/start.js",
    "test": "node scripts/test.js",
    "test:lib": "jest './libs/*' --colors --coverage --watchAll"
  },
