{
  "name": "fritz-box-challenge",
  "version": "1.0.0",
  "repository": "git@github.com:maurer2/fritz-box-challenge.git",
  "license": "MIT",
  "author": "Martin",
  "main": "index.js",
  "packageManager": "pnpm@10.26.1",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dump-box-data": "node './scripts/dump-box-data.ts'",
    "lint:style": "stylelint 'src/**/*.{css,js,jsx}'; exit 0",
    "lint:ts": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write './**/*.{ts,tsx,js,jsx,json}'",
    "format-and-lint": "pnpm run format && npm run lint:style && npm run lint:ts:fix",
    "start": "VITE_APP_MODE=prod vite",
    "dev": "VITE_APP_MODE=dev vite",
    "test:schema": "vitest './src/schema/boxFields/box.schema.ts'",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@js-temporal/polyfill": "^0.5.1",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-router": "^1.133.28",
    "@tanstack/router-generator": "^1.133.28",
    "@tanstack/router-plugin": "^1.133.28",
    "dotenv": "^16.6.1",
    "modern-normalize": "^3.0.1",
    "node-fetch": "^3.3.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "styled-components": "^6.1.19",
    "ts-pattern": "^5.8.0",
    "type-fest": "^5.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tanstack/devtools-vite": "^0.3.10",
    "@tanstack/react-devtools": "^0.7.8",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router-devtools": "^1.133.28",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^5.1.0",
    "browserslist": "^4.27.0",
    "eslint": "^8.57.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "jest-resolve": "^30.2.0",
    "jest-styled-components": "^7.2.0",
    "jsdom": "^27.3.0",
    "msw": "^2.11.6",
    "postcss": "^8.5.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.2.0",
    "postcss-normalize": "^12.0.0",
    "postcss-preset-env": "^9.6.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^6.0.4",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^4.0.2"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "pnpm": {
    "supportedArchitectures": {
      "cpu": [
        "x64",
        "arm64"
      ],
      "os": [
        "linux",
        "darwin"
      ]
    }
  }
}
