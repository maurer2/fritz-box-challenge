{
  "name": "fritz-box-challenge",
  "version": "1.0.0",
  "repository": "git@github.com:maurer2/fritz-box-challenge.git",
  "license": "MIT",
  "author": "Martin",
  "main": "index.js",
  "packageManager": "pnpm@8.15.5",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "node scripts/build.js",
    "dump-box-data": "tsx ./scripts/dump-box-data.ts",
    "lint:style": "stylelint 'src/**/*.{css,js,jsx}'; exit 0",
    "lint:ts": "eslint 'src/**/*.{ts,tsx}'",
    "lint:ts:fix": "pnpm run lint:ts -- --fix",
    "format": "prettier --write './**/*.{ts,tsx,js,jsx,json}'",
    "format-and-lint": "pnpm run format && npm run lint:style && npm run lint:ts:fix",
    "start": "VITE_APP_MODE=prod vite",
    "start:demo": "VITE_APP_MODE=dev vite",
    "test": "node scripts/test.js",
    "test:schema": "vitest './src/schema/boxFields/box.schema.ts'",
    "test:types": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.9",
    "@tanstack/react-query-devtools": "^5.28.10",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@zodios/core": "^10.9.6",
    "browserslist": "^4.23.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "fetch-mock": "^9.11.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "jest-resolve": "^29.7.0",
    "lodash": "^4.17.21",
    "modern-normalize": "^2.0.0",
    "msw": "^2.2.13",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^8.1.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^9.5.3",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "1.x",
    "styled-components": "^6.1.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/react-transition-group": "^4.4.10",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest-styled-components": "^7.2.0",
    "stylelint": "^16.3.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^6.0.4",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
